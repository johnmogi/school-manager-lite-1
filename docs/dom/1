					<div class="media-states"><strong>משמש כ:</strong> {{ data.mediaStates }}</div>
				<# } #>

				<# if ( ! data.uploading && data.can.remove ) { #>
											<button type="button" class="button-link delete-attachment">למחוק לצמיתות</button>
									<# } #>

